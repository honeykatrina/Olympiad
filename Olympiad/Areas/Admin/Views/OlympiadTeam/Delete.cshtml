@model Olympiad.Areas.Admin.Models.OlympiadTeamViewModel

@{
    ViewBag.Title = "Удаление результатов команды";
}

<h3>Вы уверены, что хотите удалить эти результаты?</h3>
<div>
    <h4>Удаление результатов команды</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt class="hidden-xs hidden-sm hidden-md hidden-lg">
            @Html.DisplayNameFor(model => model.OlympiadTeamID)
        </dt>

        <dd class="hidden-xs hidden-sm hidden-md hidden-lg">
            @Html.DisplayFor(model => model.OlympiadTeamID)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.OlympiadID)
        </dt>

        <dd>
            @ViewBag.Olympiad
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TeamID)
        </dt>

        <dd>
            @ViewBag.Team
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.TeamPlace)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TeamPlace)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.InstructorID)
        </dt>

        <dd>
            @ViewBag.Instructor
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-default" /> |
            @Html.ActionLink("Назад к списку", "Index")
        </div>
    }
</div>
